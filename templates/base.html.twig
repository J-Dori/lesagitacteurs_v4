<!DOCTYPE html>
<html lang="{{ app.request.attributes.get('_locale') }}">
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{% block title %}{{ app.request.attributes.get('site_title') }}{% endblock %}</title>
    {# <link rel="icon" href="{{ asset("images/smcs_logo.png") | imagine_filter('favicon') }}"> #}

    {% block easy_page_head_metas %}{% endblock %}
    {% block stylesheets %}
        <link rel="stylesheet" href="/css/style.css">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css" integrity="sha512-KfkfwYDsLkIlwQp6LFnl8zNdLGxu9YAA1QvwINks4PhcElQSvqcyVLLD9aMhXd13uQjoXtEKNosOWaZqXgel0g==" crossorigin="anonymous" referrerpolicy="no-referrer" />
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js" integrity="sha512-894YE6QWD5I59HgZOGReFYm4dnWc1Qt5NtvYSaNcOP+u1T9qYdvdihz0PPSiiqn/+/3e7Jo4EaG7TubfWGUrMQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
        <link rel="stylesheet" href="https://cdn.datatables.net/1.10.25/css/jquery.dataTables.min.css">
        <script src="https://cdn.datatables.net/1.10.25/js/jquery.dataTables.min.js"></script>
    {% endblock %}
</head>
<body>
    {% block navbar %}
        {# {{ render_esi(controller('App\\Controller\\NavBarController::loadNavBar')) }} #}
    {% endblock %}
    <div class="container mx-auto font-serif">
        {% block container %}
            {% block easy_page_page_header %}{% endblock %}
            <div class="text-uppercase mt-3">
                {% block easy_page_breadcrumbs %}{% endblock %}
            </div>
            {% block easy_page_page_content %}
                <main>
                    {% block easy_page_pages_list %}{% endblock %}
                </main>
            {% endblock %}
        {% endblock %}
    </div>

    {# Modal Static frame tag #}
    {# {% include '_modal.html.twig' %} #}

    {% block footer %}
        <footer class="bg-gray-800 py-10 pt-4 pb-2 font-serif">
            <div class="flex flex-wrap lg:justify-around sm:justify-center lg:flex-row flex-col">
                <div id="footer__subscribe" class="my-4 flex-1 text-center">
                    <h4 class="mb-3">NEWSLETTER</h4>
                    <form action="{{ path('app_home') }}" method="post">
                        <input type="mail" placeholder="E-mail" class="p-2 w-80 row mx-auto">
                        <button type="submit" class="btn btn-md">Souscrire</button>
                    </form>
                </div>
                <div id="footer__site_map_links" class="my-4 mx-0 flex-1">
                    <div class="flex flex-wrap text-center lg:items-start items-center justify-center lg:flex-row flex-col-reverse">
                        <div class="flex-1 mr-2">
                            {# {{ easy_menu('footer-links-col-1', { 'template': 'components/menus/links_uppercase_block.html.twig' }) }} #}
                        </div>
                        <div class="flex-1">
                            {# {{ easy_menu('footer-links-col-2', { 'template': 'components/menus/links_uppercase_block.html.twig' }) }} #}
                        </div>
                    </div>
                </div>
                <div id="footer__social" class="my-4 flex-1 text-center text-white">
                    <h4>TROUVEZ NOUS SUR</h4>
                    <div class="col">
                        {# {{ easy_menu('footer-links-media', { 'template': 'components/menus/links_inline_icon_only.html.twig' }) }} #}
                    </div>
                    <h5 class="mt-6">CONTACT</h5>
                    <p><a href="mailto:info@smcs.com">info@smcs.com</a></p>
                </div>
            </div>
            <div class="flex flex-wrap justify-center text-sm">
                <small class="mx-4"><a class="text-gray-600" href="{{ path('app_policy') }}">POLITIQUE DE CONFIDENTIALITÃ‰</a></small>
                <small class="mx-4"><a class="text-gray-600" href="{{ path('app_home') }}">COOKIES</a></small>
            </div>
        </footer>
       
    {% endblock %}
    {% block javascripts %}
        <script src="/js/jscript.js"></script>
        {# {{ easy_editor_assets() }}
        {{ easy_block_assets() }} #}
    {% endblock %}
</body>
</html>
